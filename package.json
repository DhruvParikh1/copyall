{
  "name": "copyall",
  "displayName": "copyall",
  "description": "Copies all file names and code inside each file to clipboard.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:copyFilesToClipboard"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "copyFilesToClipboard",
        "title": "Copy file names and contents to clipboard"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.78.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "clipboardy": "^2.3.0"
  }
}
